doctype html
html.standalone
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0')
    title #{object.name}
    if process.env.NODE_ENV === 'development'
      link(rel='stylesheet', href=baseUrl_https + '/css/vendor/material/material.brown-blue.min-1.3.0.css', type='text/css', data-integrity='', data-crossorigin='anonymous')
      link(rel='stylesheet', href=baseUrl_https + '/css/t6app.css', type='text/css', data-integrity='', data-crossorigin='anonymous')
      link(rel='stylesheet', href=baseUrl_https + '/css/vendor/Leaflet/leaflet-1.6.0.css')
      script(type='application/javascript', src=baseUrl_https + '/js/vendor/material/material.js')
      script(type='application/javascript', src=baseUrl_https + '/js/vendor/mdl/mdl-selectfield.min.js')
      script(type='application/javascript', src=baseUrl_https + '/js/t6show.js?build=' + t6BuildVersion)
    else
      link(rel='stylesheet', href=baseUrl_https + '/css/t6app.min.css', type='text/css', data-integrity='', data-crossorigin='anonymous')
      script(type='application/javascript', src=baseUrl_https + '/js/t6show-min.js?build=' + t6BuildVersion)
  body
    #app
    script.
      window.onload = () => { document.getElementById('app').innerHTML = materializeLight(!{ui}); componentHandler.upgradeDom(); }